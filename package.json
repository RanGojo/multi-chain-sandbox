{
  "name": "@phantom-labs/multi-chain-sandbox",
  "version": "1.0.0",
  "description": "The official CodeSandbox for learning how to interact with Phantom Wallet.",
  "license": "MIT",
  "keywords": [
    "phantom",
    "phantom wallet",
    "phantom-wallet",
    "codesandbox",
    "solana",
    "ethereum",
    "crypto",
    "blockchain",
    "love"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "@solana/web3.js": "^1.90.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-app-rewired": "^2.2.1",
    "styled-components": "^5.3.5",
    "rango-sdk-basic": "^0.1.46",
    "bignumber.js": "^9.1.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.25",
    "typescript": "^4.4.3",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "process": "^0.11.10",
    "source-map-loader": "^4.0.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.1",
    "webpack": "^5.88.0"
  },
  "scripts": {       
    "build": "tsc --build --verbose && react-app-rewired build",
    "clean": "shx mkdir -p build lib && shx rm -rf build lib",
    "lint": "prettier --check 'src/{*,**/*}.{ts,tsx,js,jsx,json}' && eslint",
    "start": "react-app-rewired start",
    "test": "CI=true react-app-rewired test --passWithNoTests",
    "test:watch": "react-app-rewired test --passWithNoTests",
    "eject": "react-scripts eject"

  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
